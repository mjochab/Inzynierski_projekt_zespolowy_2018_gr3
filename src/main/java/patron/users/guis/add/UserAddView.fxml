<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="-Infinity" minWidth="300.0" spacing="7.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane fx:id="formPane" VBox.vgrow="ALWAYS" />
      <HBox alignment="TOP_RIGHT" spacing="7.0">
         <children>
            <Button mnemonicParsing="false" onAction="#cancel" text="%view.button.cancel" />
            <Button mnemonicParsing="false" onAction="#save" text="%view.button.save" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
