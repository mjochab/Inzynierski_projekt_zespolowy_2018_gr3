<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.191" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="table" focusTraversable="false" minWidth="450.0" prefHeight="250.0" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
          <placeholder>
              <Label text="%view.table.placeholder.no_content_in_table" />
          </placeholder>
        <columns>
          <TableColumn fx:id="columnLogin" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="%view.table.column.login" />
            <TableColumn fx:id="columnManager" maxWidth="-1.0" minWidth="120.0" prefWidth="-1.0" text="%view.table.column.manager" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="TOP_RIGHT" spacing="7.0">
         <children>
            <Button focusTraversable="false" mnemonicParsing="false" onAction="#cancel" text="%view.button.cancel" />
            <Button focusTraversable="false" mnemonicParsing="false" onAction="#newUser" text="%view.button.new_user" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
